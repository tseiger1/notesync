{
  "compilerOptions": {
    "baseUrl": "./src",
    "target": "es2020",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "moduleResolution": "node",
    "types": ["node", "obsidian"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "outDir": "out"
  },
  "include": ["src/**/*.ts"]
}
